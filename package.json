    "@nuxtjs/style-resources": "^0.1.2",
    "axios": "^0.18.0",
    "bezier-easing": "^2.0.3",
    "clipboard": "^1.7.1",
    "consola": "^2.3.2",
    "cross-env": "^5.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "gravatar": "^1.6.0",
    "highlight.js": "^9.12.0",
    "howler": "^2.0.5",
    "kewler": "^1.0.8",
    "koa": "^2.6.2",
